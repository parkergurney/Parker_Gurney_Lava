generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        String   @id @default(cuid())
  name      String
  imageUrl  String
  addedAt DateTime @default(now())
	updatedAt DateTime @default(now())
  size Int
	stock Int
	orders Int
}